{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/display-name": [
      0,
      {
        "ignoreTranspilerName": false
      }
    ],
    "no-console": 0,
    "comma-dangle": 1,
    "no-undef": 1,
    "no-extra-semi": 1,
    "no-extra-parens": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-empty": 1,
    "no-use-before-define": 0,
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "eol-last": 0,
    "default-case": 1,
    "quotes": [
      0,
      "single",
      "avoid-escape"
    ]
  },
  "globals": {
  }
}
